{
    "languageserver": {
        "haskell": {
            "command": "haskell-language-server-wrapper",
            "args": ["--lsp"],
            "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
            "filetypes": ["haskell", "lhaskell"]
        }
    },
    "cSpellExt.enableDictionaries": ["spanish"],
    "cSpell.language": "en,es",
    "cSpell.userWords": [
      "Brack",
      "Commandline",
      "Cond",
      "DOUBLECOLON",
      "Dalmau",
      "Eidenbenz",
      "Guillem",
      "Hasher",
      "Initialises",
      "Ints",
      "Nonbool",
      "Nonmatching",
      "Nonsubtype",
      "Parenthesised",
      "Readline",
      "Sanitises",
      "Thicc",
      "arity",
      "backends",
      "bndvars",
      "builtins",
      "bytecode",
      "calloc",
      "codegen",
      "codespan",
      "curlyc",
      "dont",
      "elsy",
      "eval",
      "exporty",
      "externy",
      "funcs",
      "ifndef",
      "importy",
      "infixl",
      "infixr",
      "libloading",
      "malloc",
      "matchy",
      "mtype",
      "nand",
      "nosave",
      "nyaaaaa",
      "optimisation",
      "optimised",
      "parens",
      "preds",
      "putch",
      "redeclared",
      "repushes",
      "roser",
      "rustyline",
      "sanitise",
      "sanitised",
      "sexpr",
      "sexpression",
      "sexpressions",
      "sexprs",
      "subfunc",
      "tailrec",
      "termcolor",
      "varname",
      "wherey",
      "youre"
    ],
    "rust-analyzer.checkOnSave.command": "clippy"
}
